{
  "source": {
    "include": ["./src", "./README.md"],
    "includePattern": "\\.(js|jsx)$",
    "exclude": ["node_modules/", "tests/", "coverage/"]
  },
  "opts": {
    "destination": "./docs/generated/"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
